<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <data noupdate="0">

        <record model="ir.actions.act_window" id="action_dashboard">
            <field name="name">Dashboard</field>
            <field name="res_model">stationlog.wizard_dashboard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="domain">[]</field>
            <field name="target">inline</field>
        </record>

        <record model="ir.actions.act_window" id="action_logbook_list">
            <field name="name">Logbooks</field>
            <field name="res_model">stationlog.logbook</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="limit">20</field>
        </record>

        <record model="ir.actions.act_window" id="action_qso_list">
            <field name="name">QSOs</field>
            <field name="res_model">stationlog.qso</field>
            <field name="view_mode">tree,form,calendar</field>
            <field name="domain">[]</field>
            <field name="limit">20</field>
        </record>

        <record model="ir.actions.act_window" id="action_contest_list">
            <field name="name">Contest</field>
            <field name="res_model">stationlog.contest</field>
            <field name="view_mode">tree,form,calendar</field>
            <field name="domain">[]</field>
            <field name="limit">20</field>
        </record>

        <record model="ir.actions.act_window" id="action_qso_import_cabrillo">
            <field name="name">Import from Cabrillo</field>
            <field name="res_model">stationlog.wizard_cabrillo_import</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="domain">[]</field>
            <field name="target">inline</field>
        </record>

    </data>
</odoo>
